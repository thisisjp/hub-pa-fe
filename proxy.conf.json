{
  "/list-providers/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug"
  },
  "/auth/*": {
    "target": "http://localhost:3000",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/auth": ""
    }
  },
  "/microservice/*": {
    "target": "http://localhost:8081",
    "secure": false,
    "logLevel": "debug"
  },
  "/ente/*": {
    "target": "http://localhost:8082",
    "secure": false,
    "logLevel": "debug"
  },
  "/service-management/*": {
    "target": "http://localhost:8083",
    "secure": false,
    "logLevel": "debug"
  },
  "/upload-payments/*": {
    "target": "http://localhost:8084",
    "secure": false,
    "logLevel": "debug"
  }
}
